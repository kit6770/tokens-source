    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Token Source",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": "npm",
            "windows": {
                "runtimeExecutable": "npm.cmd"
            },
            "args": [
                "run",
                "dev",
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "console": "integratedTerminal",
            "protocol": "auto",
            "restart": true,
            "port": 3002,
            "internalConsoleOptions": true,
        },
    ]
}